<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter>
  <LastVersion value="-1" relOp="NEQ"/>
  <And>
    <Bug pattern="MS_PKGPROTECT"/>
    <Class name="eu.unicore.uas.security.vo.VOCommonUtils"/>
    <Field name="mappings"/>
  </And>
  <And>
    <Or>
      <Bug pattern="OBL_UNSATISFIED_OBLIGATION"/>
      <Bug pattern="OS_OPEN_STREAM"/>
    </Or>
    <Class name="eu.unicore.uas.security.vo.PullTester"/>
    <Method name="checkUASConfig"/>
  </And>
</FindBugsFilter>
